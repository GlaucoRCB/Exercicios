algoritmo "AntecessorSucessor"
var
   antecessor,sucessor,num:inteiro
inicio
      Escreva("Digite um número: ")
      leia(num)
      antecessor <- (num - 1)
      sucessor <- (num + 1)
      Escreval("O antecessor de",num," é", antecessor)
      Escreval("O sucessor de",num," é", sucessor)
fimalgoritmo