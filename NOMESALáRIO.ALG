algoritmo "NomeSalário"
var
   nome:caractere
   salario:real
inicio
      Escreva("Digite o nome do funcionário: ")
      leia(nome)
      Escreva("Digite o salário do funcionário: ")
      leia(salario)
      limpatela
      Escreval("Nome do funcionário: ",nome)
      Escreval("Salário:",salario)
      Escreva("O funcionário, ",nome," tem um salário de", salario," em janeiro.")

fimalgoritmo