algoritmo "ContagemAteNumEscolhido"
var
   contagem,escolhido:inteiro
inicio
      Escreva("Digite um número: ")
      leia(escolhido)
      contagem <- 1
      enquanto (contagem <= escolhido) faca
               escreva(contagem)
               contagem <- contagem + 1
      fimenquanto
      escreva(" Acabou!")
fimalgoritmo