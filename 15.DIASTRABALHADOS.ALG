algoritmo "DiasTrabalhados"
var
   diasTrabalhados:inteiro
   salario:real
inicio
      Escreva("Quantos dias o funcionário trabalhou?")
      leia(diasTrabalhados)
      salario <- (diasTrabalhados * 8 * 25)
      Escreva("O salário do funcionário é de",salario," de acordo com os dias trabalhados")
fimalgoritmo